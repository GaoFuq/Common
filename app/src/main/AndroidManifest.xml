            android:launchMode="singleTop" />
        <activity android:name=".activitytest.TestActivity1" />
        <activity
            android:name=".activitytest.TestActivity2"
            android:theme="@style/Base.Theme.AppCompat.Dialog" />
        <activity
            android:name=".activitytest.TestActivity3"
            android:theme="@style/translucent" />
        <activity android:name=".zxing.ZXingActivity" />
        <activity android:name=".autowired.AutoWiredActivity" />
        <activity
            android:name=".activitytest.LaunchModeActivity1"
